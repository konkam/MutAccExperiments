@article{JAT:JAT1660,
  title = {Triclosan: {{Environmental}} Exposure, Toxicity and Mechanisms of Action},
  author = {Dann, A. B. and Hontela, Alice},
  year = {2011},
  journal = {Journal of Applied Toxicology},
  volume = {31},
