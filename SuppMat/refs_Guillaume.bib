@article{leeBayesianFeatureAllocation2015,
  title = {A {{Bayesian}} Feature Allocation Model for Tumor Heterogeneity},
  author = {Lee, Juhee and M{\"u}ller, Peter and Gulukota, Kamalakar and Ji, Yuan},
