@article{konkamkingApproximateFilteringDiscrete2024,
  title = {Approximate Filtering via Discrete Dual Processes},
  author = {Kon Kam King, Guillaume and Pandolfi, Andrea and Piretto, Marco and Ruggiero, Matteo},
