@article{Jaloustre2011,
  title = {Bayesian Modeling of {{Clostridium}} Perfringens Growth in Beef-in-Sauce Products},
  author = {Jaloustre, S. and Cornu, Marie and Morelli, E. and No??l, V. and {Delignette-Muller}, Marie Laure},
  year = {2011},
