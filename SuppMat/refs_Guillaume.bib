@article{guhaPosteriorContractionParameters2021,
  title = {On Posterior Contraction of Parameters and Interpretability in {{Bayesian}} Mixture Modeling},
  author = {Guha, Aritra and Ho, Nhat and Nguyen, XuanLong},
  year = {2021},
  month = nov,
  journal = {Bernoulli},
